{"ast":null,"code":"export const showNotification=(title,message)=>{if(window.Notification&&Notification.permission==='granted'){new Notification(title,{body:message});}else if(window.Notification&&Notification.permission!=='denied'){Notification.requestPermission().then(permission=>{if(permission==='granted'){new Notification(title,{body:message});}});}};","map":{"version":3,"names":["showNotification","title","message","window","Notification","permission","body","requestPermission","then"],"sources":["/mnt/development/pop-dev-assistant/vscode-extension/src/payments/pop-payment-module/src/utils/notifications.ts"],"sourcesContent":["export const showNotification = (title: string, message: string) => {\n    if (window.Notification && Notification.permission === 'granted') {\n        new Notification(title, { body: message });\n    } else if (window.Notification && Notification.permission !== 'denied') {\n        Notification.requestPermission().then(permission => {\n            if (permission === 'granted') {\n                new Notification(title, { body: message });\n            }\n        });\n    }\n};"],"mappings":"AAAA,MAAO,MAAM,CAAAA,gBAAgB,CAAGA,CAACC,KAAa,CAAEC,OAAe,GAAK,CAChE,GAAIC,MAAM,CAACC,YAAY,EAAIA,YAAY,CAACC,UAAU,GAAK,SAAS,CAAE,CAC9D,GAAI,CAAAD,YAAY,CAACH,KAAK,CAAE,CAAEK,IAAI,CAAEJ,OAAQ,CAAC,CAAC,CAC9C,CAAC,IAAM,IAAIC,MAAM,CAACC,YAAY,EAAIA,YAAY,CAACC,UAAU,GAAK,QAAQ,CAAE,CACpED,YAAY,CAACG,iBAAiB,CAAC,CAAC,CAACC,IAAI,CAACH,UAAU,EAAI,CAChD,GAAIA,UAAU,GAAK,SAAS,CAAE,CAC1B,GAAI,CAAAD,YAAY,CAACH,KAAK,CAAE,CAAEK,IAAI,CAAEJ,OAAQ,CAAC,CAAC,CAC9C,CACJ,CAAC,CAAC,CACN,CACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}